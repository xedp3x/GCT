<p id="notice"><%= notice %></p>

<dl class="dl-horizontal lage-dt">
	<% done_list = {}%>
	<% @group.must.each {|m| %>
	<%= render('show', :m => m) if done_list[m.id].nil?%>
	<% done_list[m.id] = true }%>

	<% @group.may.each {|m| %>
	<%= render('show', :m => m) if done_list[m.id].nil?%>
	<% done_list[m.id] = true }%>
</dl>

